<%include ./partials/header %>
<a class="ui basic black button" href="/company_info/query" style="margin-left: 5px;">Go Back</a>
<div class="ui main text container">
        <div class="ui top attached segment">
            <div class="ui divided items">
                    <%for(var i = 0; i < dataResult.length; i++){%>
                        <div class="ui list">
                            <div class="item">
                            <p><strong>Name:</strong><%=dataResult[i].CompanyName%></p>
                            </div>
                            <div class="item">
                            <p> <strong>Company Address :</strong>  <%=dataResult[i].CompanyAddr.Line1%>,<%=dataResult[i].CompanyAddr.City%></p>
                            </div>
                            <div class="item">
                            <p><strong>Country:</strong> <%=dataResult[i].Country%></p>
                            </div>
                            <div class="item">
                            <p><strong>Email:</strong> <%=dataResult[i].Email.Address%></p>
                            </div>
                            <div class="item">
                            <p><strong>Sync Token:</strong> <%=dataResult[i].SyncToken%></p>
                            </div>
                        </div>
                        <hr>
                    <%} %>
            </div>
        </div>
</div>

<a class="ui basic black button" href="/company_info/query" style="margin-left: 5px;">Go Back</a>
<%include ./partials/footer %>
